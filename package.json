{
  "name": "vite-vue-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "build:staging": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "analyze": "vite build --mode analyze",
    "preview": "http-server ./dist -g",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .vue,.ts,.tsx .",
    "lint:js--fix": "npm run lint:js -- --fix",
    "lint:style": "stylelint \"src/**/*.{vue,css,scss,postcss}\"",
    "lint:style--fix": "npm run lint:style -- --fix",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "precommit": "lint-staged",
    "postinstall": "simple-git-hooks",
    "updatehooks": "git config core.hooksPath .git/hooks/ && rimraf -rf .git/hooks && npx simple-git-hooks"
  },
  "dependencies": {
    "@vant/touch-emulator": "^1.4.0",
    "@vueuse/core": "^10.8.0",
    "axios": "^1.4.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "lottie-web": "^5.12.2",
    "mockjs": "^1.1.0",
    "pinia": "^2.1.7",
    "vant": "^4.8.5",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@types/lodash-es": "^4.17.12",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^20.3.3",
    "@types/postcss-pxtorem": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-legacy": "^5.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "cz-git": "^1.8.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.20.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.22.0",
    "lint-staged": "^11.2.3",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "postcss-pxtorem": "^6.0.0",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.63.6",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.6.2",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^5.2.2",
    "unplugin-svg-component": "^0.7.4",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^3.0.1",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=16.15.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run precommit",
    "commit-msg": "npx --no-install commitlint --edit \"$1\""
  },
  "lint-staged": {
    "*.{vue,ts,tsx}": "eslint",
    "*.{vue,css,less,scss,postcss}": "stylelint"
  }
}
