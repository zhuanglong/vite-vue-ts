{
  "name": "vite-vue-ts",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/zhuanglong/vite-vue-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhuanglong/vite-vue-ts.git"
  },
  "bugs": {
    "url": "https://github.com/zhuanglong/vite-vue-ts/issues"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite --mode dev",
    "build:uat": "vite build --mode uat",
    "build:prod": "vite build --mode prod",
    "analyze": "vite build --mode analyze",
    "preview": "http-server ./dist -c-1",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --cache",
    "lint:js--fix": "npm run lint:js -- --fix",
    "lint:style": "stylelint \"src/**/*.{css,scss,vue}\" --cache",
    "lint:style--fix": "npm run lint:style -- --fix",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "precommit": "lint-staged",
    "postinstall": "simple-git-hooks",
    "updatehooks": "git config core.hooksPath .git/hooks/ && rimraf -rf .git/hooks && npx simple-git-hooks"
  },
  "dependencies": {
    "@vant/touch-emulator": "^1.4.0",
    "@vueuse/core": "^10.8.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "lottie-web": "^5.12.2",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "postcss": "^8.5.6",
    "vant": "^4.8.5",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.0",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@faker-js/faker": "^9.9.0",
    "@stylistic/stylelint-config": "^2.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.3.3",
    "@vant/auto-import-resolver": "^1.3.0",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "autoprefixer": "^10.4.14",
    "cz-git": "^1.8.0",
    "eslint": "^9.29.0",
    "lint-staged": "^16.1.5",
    "postcss-html": "^1.8.0",
    "postcss-mobile-forever": "^5.0.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass-embedded": "^1.90.0",
    "simple-git-hooks": "^2.13.1",
    "stylelint": "^16.20.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-scss": "^6.12.1",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-svg-component": "^0.12.1",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^7.1.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-fake-server": "^2.2.0",
    "vite-plugin-html": "^3.2.0",
    "vue-tsc": "^3.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run precommit",
    "commit-msg": "npx --no-install commitlint --edit \"$1\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --cache",
    "*.{css,scss,vue}": "stylelint --cache"
  }
}
