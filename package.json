{
  "name": "vite-vue-ts",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/zhuanglong/vite-vue-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhuanglong/vite-vue-ts.git"
  },
  "bugs": {
    "url": "https://github.com/zhuanglong/vite-vue-ts/issues"
  },
  "engines": {
    "node": ">=16.15.1"
  },
  "scripts": {
    "dev": "vite --mode dev",
    "build:uat": "vite build --mode uat",
    "build:prod": "vite build --mode prod",
    "analyze": "vite build --mode analyze",
    "preview": "http-server ./dist -g",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --cache",
    "lint:js--fix": "npm run lint:js -- --fix",
    "lint:style": "stylelint \"src/**/*.{css,scss,vue}\" --cache",
    "lint:style--fix": "npm run lint:style -- --fix",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "precommit": "lint-staged",
    "postinstall": "simple-git-hooks",
    "updatehooks": "git config core.hooksPath .git/hooks/ && rimraf -rf .git/hooks && npx simple-git-hooks"
  },
  "dependencies": {
    "@vant/touch-emulator": "^1.4.0",
    "@vueuse/core": "^10.8.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "lottie-web": "^5.12.2",
    "mockjs": "^1.1.0",
    "pinia": "^3.0.3",
    "vant": "^4.8.5",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@iconify/json": "^2.2.189",
    "@stylistic/stylelint-config": "^2.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^20.3.3",
    "@unocss/eslint-plugin": "^66.2.1",
    "@unocss/preset-icons": "^0.58.5",
    "@unocss/preset-rem-to-px": "^0.58.5",
    "@unocss/reset": "^0.58.5",
    "@unocss/transformer-attributify-jsx": "^0.58.5",
    "@unocss/transformer-directives": "^0.58.5",
    "@unocss/transformer-variant-group": "^0.58.5",
    "@vitejs/plugin-legacy": "^5.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "cz-git": "^1.8.0",
    "eslint": "^9.29.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^11.2.3",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "postcss-mobile-forever": "^5.0.0",
    "postcss-scss": "^4.0.3",
    "rimraf": "^5.0.5",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "1.63.6",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^16.20.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-scss": "^6.12.1",
    "typescript": "5.1.6",
    "unocss": "^0.58.5",
    "unplugin-svg-component": "^0.12.1",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^3.0.1",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run precommit",
    "commit-msg": "npx --no-install commitlint --edit \"$1\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "eslint --cache",
    "*.{css,scss,vue}": "stylelint --cache"
  }
}
